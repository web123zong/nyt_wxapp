}
.course-img{
  border: 1rpx solid #000;
  border-radius: 20rpx;
  float: right;
  width: 35%;
  height: 80%;
  margin: 25rpx 40rpx 0 0;
}
.progress{
  float: left;
  height: 100rpx;
}